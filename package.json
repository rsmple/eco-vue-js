{
  "name": "@whitespots/ui-kit",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest --environment jsdom --root src/",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "peerDependencies": {
    "vue": "^3.2.0",
    "vue-router": "^4.1.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "1.1.4",
    "@types/jsdom": "20.0.1",
    "@types/node": "18.11.12",
    "@types/postcss-import": "14.0.0",
    "@vitejs/plugin-vue": "4.0.0",
    "@vue/eslint-config-prettier": "7.0.0",
    "@vue/eslint-config-typescript": "11.0.0",
    "@vue/test-utils": "2.2.6",
    "@vue/tsconfig": "0.1.3",
    "autoprefixer": "10.4.13",
    "eslint": "8.22.0",
    "eslint-plugin-vue": "9.3.0",
    "jsdom": "20.0.3",
    "npm-run-all": "4.1.5",
    "path": "0.12.7",
    "postcss": "8.4.21",
    "postcss-import": "15.1.0",
    "prettier": "2.7.1",
    "tailwindcss": "3.2.4",
    "typescript": "4.7.4",
    "vite": "4.0.0",
    "vite-plugin-dts": "1.7.1",
    "vite-svg-loader": "^4.0.0",
    "vitest": "0.25.6",
    "vue-tsc": "1.0.12"
  },
  "type": "module",
  "files": [
    "dist",
    "tailwind-base"
  ],
  "main": "./dist/main.js",
  "module": "./dist/main.js",
  "exports": {
    ".": {
      "import": "./dist/main.js"
    },
    "./tailwind-base/index.cjs": {
      "require": "./tailwind-base/index.cjs"
    },
    "./dist/assets/icons/default/*": {
      "import": "./dist/assets/icons/default/*.svg.js"
    },
    "./dist/components/Button/WButton.vue": {
      "import": "./dist/components/Button/WButton.vue.js"
    },
    "./dist/components/Button/WButtonAction.vue": {
      "import": "./dist/components/Button/WButtonAction.vue.js"
    },
    "./dist/components/FieldWrapper/WFieldWrapper.vue": {
      "import": "./dist/components/FieldWrapper/WFieldWrapper.vue.js"
    },
    "./dist/components/Input/WInput.vue": {
      "import": "./dist/components/Input/WInput.vue.js"
    },
    "./dist/components/Input/WInputAsync.vue": {
      "import": "./dist/components/Input/WInputAsync.vue.js"
    }
  },
  "types": "./dist/main.d.ts"
}