    "rollup-plugin-watch": "1.0.5",
    "tailwindcss": "3.4.16",
    "typescript": "5.6.3",
    "vite": "6.1.0",
    "vite-plugin-dts": "4.5.0",
    "vite-svg-loader": "5.1.0",
    "vue-tsc": "2.1.10"
